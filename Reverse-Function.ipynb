    "def rvrs(l1):\n",
    "    l1.reverse()\n",
    "    for i in range(len(l1)):\n",
    "        if type(l1[i])==list:\n",
    "            rvrs(l1[i])\n",
    "        else:\n",
    "            continue\n",
    "    return l1\n",
    "\n",
    "l=[[1, 2], [3, [4]], [5, [6, 7]]]\n",
    "print(rvrs(l))\n"
   ]
  }
